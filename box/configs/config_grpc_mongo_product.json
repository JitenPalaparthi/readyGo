{
    "version": "0.1",
    "kind": "grpc",
    "project": "product_sample",
    "databaseSpec": {
      "kind": "mongo",
      "connectionString": "mongodb://localhost:27017",
      "name": "sample"
    },
    "apiSpec": {
      "kind": "grpc",
      "port": "50057",
      "version": "v1"
    },
    "models": [
      {
        "name": "product",
        "fields": [
          {
            "name": "name",
            "type": "string",
            "isKey": true
          },
          {
            "name": "description",
            "type": "string",
            "validateExp": "[a-zA-Z0-9]"
          },
          {
            "name": "refNo",
            "type": "long"
          },
          {
            "name": "hasStock",
            "type": "bool"
          },
          {
            "name": "status",
            "type": "string"
          },
          {
            "name": "lastModified",
            "type": "string"
          }
        ]
      }
    ]
  }