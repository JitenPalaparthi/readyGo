{
  "version": "0.1",
  "kind": "http",
  "project": "contacts",
  "databaseSpec": {
    "kind": "mongo",
    "connectionString": "mongodb://localhost:27017",
    "name": "contacts"
  },
  "apiSpec": {
    "kind": "http",
    "port": "50058",
    "version": "v1"
  },
  "models": [
    {
      "name": "person",
      "fields": [
        {
          "name": "name",
          "type": "string",
          "isKey": true
        },
        {
          "name": "email",
          "type": "string",
          "isKey": true,
          "validateExp": "^[a-zA-Z0-9.!#$%&'*+\\\\/=?^_\\\\`{|}~-]+@[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?(?:\\\\.[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?)*$"
        },
        {
          "name": "gender",
          "type": "string"
        },
        {
          "name": "contact",
          "type": "long"
        },
        {
          "name": "status",
          "type": "global.GetDefaultStr(`active`)"
        },
        {
          "name": "lastModified",
          "type": "global.GetCurrentDateTimeInStr()"
        },
        {
          "name": "Address",
          "type": "Address"
        }
      ]
    },
    {
      "name": "Address",
      "fields": [
        {
          "name": "addressline",
          "type": "string"
        },
        {
          "name": "street",
          "type": "string"
        },
        {
          "name": "state",
          "type": "string"
        },
        {
          "name": "country",
          "type": "string"
        },
        {
          "name": "zip",
          "type": "string"
        }
      ]
    }
  ]
}