{
  "version": "0.1",
  "kind": "grpc",
  "project": "grpcmongoproductSample",
  "databaseSpec": {
    "kind":"nosql",
    "name": "mongo",
    "connectionString": "mongodb://localhost:27017",
    "dbName": "sample"
  },
  "apiSpec": {
    "kind": "grpc",
    "port": "50057",
    "version": "v1"
  },
  "models": [
    {
      "name": "person",
      "fields": [
        {
          "name": "name",
          "type": "string",
          "isKey": true
        },
        {
          "name": "govtNo",
          "type": "long"
        },
        {
          "name": "isIndian",
          "type": "bool"
        },
        {
          "name": "email",
          "type": "string",
          "validateExp": "[a-zA-Z0-9]",
          "isKey": true
        },
        {
          "name": "mobile",
          "type": "string"
        },
        {
          "name": "status",
          "type": "string"
        },
        {
          "name": "lastModified",
          "type": "global.GetCurrentDateTimeInStr()"
        }
      ]
    }
  ]
}