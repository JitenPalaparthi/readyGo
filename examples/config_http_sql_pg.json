{
  "version": "0.1",
  "kind": "http",
  "project": "sqlpgsample",
  "databaseSpec": {
    "kind": "sql",
    "name":"postgres",
    "connectionString": "user=jiten  password=admin host=localhost port=5432 dbname=sample",
    "dbName": "sample"
  },
  "apiSpec": {
    "kind": "http",
    "port": "8083",
    "version": "v1"
  },
  "models": [
    {
      "name": "person",
      "fields": [
        {
          "name": "name",
          "type": "string",
          "isKey": true
        },
        {
          "name": "email",
          "type": "string",
          "validateExp": "[a-zA-Z0-9]",
          "isKey": true
        },
        {
          "name": "mobile",
          "type": "string"
        },
        {
          "name": "status",
          "type": "string"
        },
        {
          "name": "lastModified",
          "type": "string"
        }
      ]
    }
  ]
}