    "apiSpec": {
        "kind": "http",
        "port": "50061",
        "version": "v1"
    },
    
    "databaseSpec": {
        "kind": "sql",
        "name":"postgres",
        "connectionString": "user=jiten  password=admin host=localhost port=5432 dbname=sample",
        "dbName": "eduempowerDb"
    },
    "messagingSpec": {
        "kind":"broker",
        "name":"nats",
